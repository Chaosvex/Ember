namespace ember.messagingv2.realm;

table RequestRealmStatus {
	// nothing!
}

table RealmStatus {
	id:uint;
	name:string;
	ip:string;
	population:float;
	type:uint;
	flags:ubyte;
	category:int;
	region:int;
}

rpc_service RealmStatusv2 {
	StatusRequest(RequestRealmStatus):RealmStatus;
}